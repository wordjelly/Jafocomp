<% @entities_by_exchange.keys.each do |exchange_name| %>
	var div_id = "#<%= exchange_name.parameterize %>_stocks";
	<% @entities_by_exchange[exchange_name][:stocks].each do |entity| %>
		<% @entity = entity %>
		<% @from = @entities_by_exchange[exchange_name][:next_from] %>
		$(div_id).append("<%= escape_javascript(render :partial => '/entities/show_summary.html.erb') %>");
	<% end %>
<% end %>